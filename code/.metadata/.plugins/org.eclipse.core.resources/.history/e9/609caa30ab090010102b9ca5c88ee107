package com.pikitus.games.go;

import com.pikitus.games.go.models.BoardModel;
import com.pikitus.games.go.models.MoveModel;

public class GoBoard 
{
	private MoveModel mMoveModel = new MoveModel();
	private BoardModel mBoardModel = new BoardModel();
	private int[][] mBoard = new int[19][19];

	public GoBoard() 
	{
		ResetBoard();
	}
	
	public BoardModel GetBoard() 
	{
		mBoardModel.SetBoard(mBoard);
		return mBoardModel;
	}
}
