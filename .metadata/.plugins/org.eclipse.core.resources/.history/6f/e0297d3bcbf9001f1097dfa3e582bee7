package sfs2x.extensions.games.chess;

import com.restfb.Facebook;

public class FriendListResult 
{
	@Facebook
	String uid;

	@Facebook
	String name;
	
	@Facebook
	String picture;

	int mRating; 

	@Override
	public String toString() 
	{
		return String.format("%s - %s - %s, %i", uid, name, picture, mRating); 	
	}
}
